pipeline {
    agent any
    parameters {
        string(name: 'from_date', defaultValue: 'March 23, 2023', description: 'From date in MM-dd-YYY format')
        string(name: 'to_date', defaultValue: 'March 27, 2023', description: 'To date in MM-dd-YYY format')
    }
    stages {
        stage('Find Files') {
            steps {
                script {
                    def output = sh(script: "find . -type f -newermt ${params.from_date} ! -newermt ${params.to_date} -exec ls -lh {} \\; | awk '{print \$5, \$9}'", returnStdout: true)
                    echo "Files created between ${params.from_date} and ${params.to_date}:"
                    echo output
                }
            }
        }
    }
}
